{
  "name": "@lewebsimple/nuxt-barebones",
  "description": "Nuxt barebones project",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "lint": "eslint --fix . && stylelint --fix \"**/*.{css,scss,vue}\"",
    "postinstall": "husky && nuxt prepare",
    "preview": "nuxt preview"
  },
  "dependencies": {
    "nuxt": "^3.13.1",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@lewebsimple/eslint-config": "^0.9.6",
    "@lewebsimple/stylelint-config": "^0.9.6",
    "eslint": "^8.57.0",
    "husky": "^9.1.5",
    "nano-staged": "^0.8.0",
    "stylelint": "^16.9.0"
  },
  "nano-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix"
  },
  "packageManager": "pnpm@9.10.0"
}